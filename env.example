DATABASE_URL=postgresql://postgres:password@host:port/railway
# Optional: disable SSL if your PG doesn't require it
# PGSSL=disable

# LLM
OPENAI_API_KEY=
# Optional model, defaults to gpt-4.1-mini
OPENAI_MODEL=gpt-4.1-mini
# Optional tuning for report generation
# Max tokens for Responses API structured output (json+markdown)
REPORT_MAX_OUTPUT_TOKENS=1400
# Max tokens for Chat Completions JSON mode
REPORT_CHAT_MAX_TOKENS=1400
# Overall timeout for report generation in ms
REPORT_TIMEOUT_MS=120000

# Optional default chat id when not provided in query
DEFAULT_CHAT_ID=
# PORT=3000

